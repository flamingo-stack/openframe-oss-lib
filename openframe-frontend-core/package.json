  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flamingo-stack/openframe-oss-lib.git",
    "directory": "openframe-frontend-core"
  }
}
