{
  "name": "@flamingo-stack/openframe-frontend-core",
  "version": "0.0.45",
  "description": "Shared design system and components for all Flamingo platforms",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src/styles",
    "tailwind.config.ts"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.cjs",
      "default": "./dist/components/index.js"
    },
    "./components/ui": {
      "types": "./dist/components/ui/index.d.ts",
      "import": "./dist/components/ui/index.js",
      "require": "./dist/components/ui/index.cjs",
      "default": "./dist/components/ui/index.js"
    },
    "./components/ui/file-manager": {
      "types": "./dist/components/ui/file-manager/index.d.ts",
      "import": "./dist/components/ui/file-manager/index.js",
      "require": "./dist/components/ui/file-manager/index.cjs",
      "default": "./dist/components/ui/file-manager/index.js"
    },
    "./components/features": {
      "types": "./dist/components/features/index.d.ts",
      "import": "./dist/components/features/index.js",
      "require": "./dist/components/features/index.cjs",
      "default": "./dist/components/features/index.js"
    },
    "./components/toast": {
      "types": "./dist/components/toast/index.d.ts",
      "import": "./dist/components/toast/index.js",
      "require": "./dist/components/toast/index.cjs",
      "default": "./dist/components/toast/index.js"
    },
    "./components/icons": {
      "types": "./dist/components/icons/index.d.ts",
      "import": "./dist/components/icons/index.js",
      "require": "./dist/components/icons/index.cjs",
      "default": "./dist/components/icons/index.js"
    },
    "./components/navigation": {
      "types": "./dist/components/navigation/index.d.ts",
      "import": "./dist/components/navigation/index.js",
      "require": "./dist/components/navigation/index.cjs",
      "default": "./dist/components/navigation/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.cjs",
      "default": "./dist/hooks/index.js"
    },
    "./nats": {
      "types": "./dist/nats/index.d.ts",
      "import": "./dist/nats/index.js",
      "require": "./dist/nats/index.cjs",
      "default": "./dist/nats/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs",
      "default": "./dist/utils/index.js"
    },
    "./styles": {
      "import": "./src/styles/index.css",
      "require": "./src/styles/index.css",
      "default": "./src/styles/index.css"
    },
    "./styles/*": "./src/styles/*",
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.cjs",
      "default": "./dist/types/index.js"
    },
    "./types/navigation": {
      "types": "./dist/types/navigation.d.ts",
      "import": "./dist/types/navigation.js",
      "require": "./dist/types/navigation.cjs",
      "default": "./dist/types/navigation.js"
    },
    "./types/announcement": {
      "types": "./dist/types/announcement.d.ts",
      "import": "./dist/types/announcement.js",
      "require": "./dist/types/announcement.cjs",
      "default": "./dist/types/announcement.js"
    },
    "./assets": {
      "types": "./dist/assets/index.d.ts",
      "import": "./dist/assets/index.js",
      "require": "./dist/assets/index.cjs",
      "default": "./dist/assets/index.js"
    },
    "./fonts": {
      "types": "./dist/fonts.d.ts",
      "import": "./dist/fonts.js",
      "require": "./dist/fonts.cjs",
      "default": "./dist/fonts.js"
    },
    "./tailwind.config.ts": {
      "types": "./dist/tailwind.config.d.ts",
      "default": "./tailwind.config.ts"
    },
    "./components/icons-v2": {
      "types": "./dist/components/icons-v2-generated/index.d.ts",
      "import": "./dist/components/icons-v2-generated/index.js",
      "require": "./dist/components/icons-v2-generated/index.cjs",
      "default": "./dist/components/icons-v2-generated/index.js"
    }
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "dev": "NODE_OPTIONS='--max-old-space-size=8192' tsup --watch",
    "build": "rm -rf dist && NODE_OPTIONS='--max-old-space-size=8192' tsup",
    "build:fast": "NODE_OPTIONS='--max-old-space-size=8192' tsup --no-dts",
    "clean": "rm -rf dist node_modules/.cache",
    "prepack": "NODE_OPTIONS='--max-old-space-size=8192' npm run build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "yalc:watch": "NODE_OPTIONS='--max-old-space-size=8192' nodemon --watch src --watch '*.json' --watch '*.ts' --watch '*.tsx' -e ts,tsx,json,css --exec 'npm run build && yalc push --changed'",
    "yalc:push": "NODE_OPTIONS='--max-old-space-size=8192' npm run build && yalc push",
    "yalc:publish": "yalc publish",
    "generate:icons": "node scripts/generate-icons.mjs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0",
    "next": "^15.0.0 || ^16.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.15",
