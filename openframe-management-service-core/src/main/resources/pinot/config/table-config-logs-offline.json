{
  "tableName": "${pinot.tables.logs.name}_OFFLINE",
  "tableType": "OFFLINE",
  "segmentsConfig": {
    "timeColumnName": "eventTimestamp",
    "timeType": "MILLISECONDS",
    "replication": "1",
    "segmentAssignmentStrategy": "BalanceNumSegmentAssignmentStrategy",
    "retentionTimeUnit": "DAYS",
    "retentionTimeValue": "3650"
  },
  "tableIndexConfig": {
    "loadMode": "MMAP",
    "nullHandlingEnabled": true,
    "optimizeDictionary": true,
    "sortedColumn": [
      "eventTimestamp"
    ],
    "bloomFilterColumns": [
      "toolEventId"
    ],
    "noDictionaryColumns": [
      "summary",
      "userId",
      "hostname",
      "organizationName"
    ],
    "invertedIndexColumns": [
      "ingestDay",
      "toolType",
      "eventType",
      "severity",
      "organizationId",
      "deviceId"
    ],
    "textIndexColumns": [
      "summary",
      "userId",
      "organizationName"
    ],
    "rangeIndexColumns": [
      "eventTimestamp"
    ]
  },
  "fieldConfigList": [
    {
      "name": "summary",
      "encodingType": "RAW",
      "indexTypes": [
        "TEXT"
      ],
      "properties": {
        "enableQueryCache": "true",
        "caseSensitive": "false"
      }
    },
    {
      "name": "userId",
      "encodingType": "RAW",
      "indexTypes": [
        "TEXT"
      ],
      "properties": {
        "enableQueryCache": "true",
        "caseSensitive": "false"
      }
    },
    {
      "name": "organizationName",
      "encodingType": "RAW",
      "indexTypes": [
        "TEXT"
      ],
      "properties": {
        "enableQueryCache": "true",
        "caseSensitive": "false"
      }
    },
    {
      "name": "hostname",
      "encodingType": "RAW",
      "indexTypes": [
        "TEXT"
      ],
      "properties": {
        "enableQueryCache": "true",
        "caseSensitive": "false"
      }
    }
  ],
  "ingestionConfig": {
    "batchIngestionConfig": {
      "segmentIngestionType": "APPEND",
      "segmentIngestionFrequency": "MONTHLY"
    }
  },
  "tenants": {
    "broker": "DefaultTenant",
    "server": "DefaultTenant"
  }
}